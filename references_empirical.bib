  Year = {2014},
  Keywords = {\#duplicate-citation-key, Acculturation, Acculturative stress, Asian Americans, P-101},
  Issn = {15322491},
  Doi = {10.3109/10826084.2013.855232},
  Abstract = {Acculturation and acculturative stress are examined as predictors of alcohol use among Asian immigrants, using the 2004 National Latino and Asian Americans Survey (NLAAS). Separate regression analyses were conducted for Chinese (n = 600), Filipino (n = 508), and Vietnamese (n = 520) immigrants. Alcohol use varied for the three groups. English proficiency was associated with drinking for all groups. Family conflict was associated with drinking for Chinese immigrants. General acculturative stress and discrimination were associated with drinking for Vietnamese immigrants. Results underscore acculturation and acculturative stress as being contributors to alcohol consumption, and the importance of considering the heterogeneity of Asian immigrants in research on their alcohol use. The study's limitations are noted. Copyright © 2014 Informa Healthcare USA, Inc.}
}


@Article{Zagefka2002,
