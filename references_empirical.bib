  Pages = {347--355},
  Title = {Can {Cultural} {Identity} {Clarity} {Protect} the {Well}-{Being} of {Latino}/a {Canadians} {From} the {Negative} {Impact} of {Race}- {Based} {Rejection} {Sensitivity}?},
  Volume = {26},
