  Keywords = {\#duplicate-citation-key, Acculturation, Activities of Daily Living, Adult, Athens, Cross-Sectional Studies, Depression, Emigrants and Immigrants, Female, Greece, Humans, Immigration, Interview, Psychological, Major Depression, Male, Psychiatric Status Rating Scales, Religion, Social Identification, Symptoms, acculturation factors, depression, immigration, symptoms},
