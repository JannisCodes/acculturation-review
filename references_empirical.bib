@Article{Huang2006,
  Author = {Huang, Shu Ling and Spurgeon, Anne},
  Journal = {Ethnicity and Health},
  Number = {4},
  Pages = {365--387},
  Title = {The mental health of {Chinese} immigrants in {Birmingham}, {UK}},
