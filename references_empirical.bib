  Volume = {44},
  Year = {2009},
  Keywords = {\#duplicate-citation-key, Acculturation, Adult, Drug Abuse, Female, HIV, HIV Infections, HIV-risk behaviors, Hispanic Americans, Humans, Interviews as Topic, Intravenous Drug Usage, Logistic Models, Male, Middle Aged, Multiculturalism, New York City, P-646, Puerto Rico, Risk Taking, Risk-Taking, Self Efficacy, Substance-Related Disorders, biculturality, injecting drug users},
