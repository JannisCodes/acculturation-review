  Author = {Irvin, Veronica L and Nichols, Jeanne F and Hofstetter, C Richard and Ojeda, Victoria D and Song, Yoon Ju and Kang, Sunny and Hovell, Melbourne F},
  Journal = {Journal of Immigrant and Minority Health},
  Month = {},
