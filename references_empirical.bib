  Keywords = {\#duplicate-citation-key, Acculturation, Adult, Conservatism, Cross-Sectional Studies, Distress, Female, Health Surveys, Human Migration, Humans, Interviews as Topic, Islam, Linear Models, Male, Mental Health, Middle Aged, Morocco, Muslim migrants, Netherlands, P-542, Psychological, Psychological Stress, Stress, Turkey, acculturation, psychological distress, traditionalism},
