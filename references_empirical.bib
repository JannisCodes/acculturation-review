  Keywords = {\#duplicate-citation-key, Acculturation, Adolescent, Adult, Bullying, Crime Victims, Emigration and Immigration, Female, Hispanic Americans, Human Females, Humans, Immigration, Interpersonal Relations, Interviews as Topic, Latino women, Latinos/Latinas, Logistic Models, Middle Aged, Retrospective Studies, Risk Factors, Self Report, Socioeconomic Factors, Stress, Psychological, Surveys and Questionnaires, United States, Victimization, Violence, Young Adult, acculturation, immigrants, interpersonal victimization},
