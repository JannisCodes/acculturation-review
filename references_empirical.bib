  Keywords = {\#duplicate-citation-key, Acculturation, Achievement, Adaptation, Psychological, Adolescent, Adolescent Psychology, Antisocial Personality Disorder, Anxiety Disorders, Asian Continental Ancestry Group, Australia, China, Chinese adolescents, Cross Cultural Differences, Cross-Cultural Comparison, Depressive Disorder, Emigrants and Immigrants, Female, Hong Kong, Human Migration, Humans, Male, Quality of Life, Self Concept, Social Values, Society, Somatoform Disorders, cross cultural differences, migrantion, societies},
