  Keywords = {\#duplicate-citation-key, Acculturation, Adolescent, Adult, Age Distribution, Educational Status, England, Exercise, Female, Germany, Human Migration, Humans, Immigration, Male, Marital Status, Motor Activity, Physical Activity, Sex Distribution, Socioeconomic Factors, Transients and Migrants, Turkey, Turkish migrants, Young Adult, acculturation, exercise, physical activity},
