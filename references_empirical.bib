  Note = {ISBN: 0022022112},
  Number = {5},
  Pages = {701--718},
  Title = {Acculturation of {Personality}: {A} {Three}-{Culture} {Study} of {Japanese}, {Japanese} {Americans}, and {European} {Americans}},
  Volume = {44},
