  Keywords = {\#duplicate-citation-key, Acculturation, Adaptation, Psychological, Adolescent, Adolescent Development, Child, Emigrants and Immigrants, Ethnic Groups, Ethnic Identity, Family, Family Relations, Female, Humans, Immigration, Longitudinal Studies, Male, Models, Statistical, Psychology, Adolescent, Self Efficacy, Self-Efficacy, acculturation, ethnic identity, family functioning, immigrant adolescents, longitudinal, self-efficacy},
