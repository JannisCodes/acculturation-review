  Volume = {34},
  Year = {2019},
  Keywords = {\#duplicate-citation-key, Adaptation, Adolescent Characteristics, Immigration, Minority Groups, P-998, Race and Ethnic Discrimination, Racial and Ethnic Groups, Sociocultural Factors, Teacher Attitudes, ethnic minority youth, immigrant adolescents, measurement equivalence, sociocultural adaptation},
