@article{ ISI:000312274400002,
Author = {Luedicke, Marius K.},
Title = {{Consumer acculturation theory: (crossing) conceptual boundaries}},
Journal = {{CONSUMPTION MARKETS \& CULTURE}},
Year = {{2011}},
Volume = {{14}},
Number = {{3, SI}},
Pages = {{223-244}},
Abstract = {{Consumer acculturation theorists have developed an insightful body of
   literature about the ways in which migrants adapt to foreign cultures
   via consumption. The present paper revisits 14 key studies from this
   field to highlight its most important contributions, critique its
   conceptual boundaries, and present cases of conceptual border crossings
   that indicate an emerging need for a broader conceptualization of the
   phenomenon. The paper closes by introducing a model that frames consumer
   acculturation as a complex system of recursive socio-cultural
   adaptation, and discusses its implications for future research.}},
