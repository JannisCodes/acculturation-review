@article{ ISI:000475724700007,
Author = {Kim, Byeong Jo and Rousseau, Denise M.},
Title = {{INTERNALIZING CAPITALIST NORMS: A GROUNDED THEORY STUDY OF HOW NORTH
   KOREAN ESCAPEES ADAPT TO WORK}},
Journal = {{ACADEMY OF MANAGEMENT DISCOVERIES}},
