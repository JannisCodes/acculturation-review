@article{ ISI:000360911300016,
Author = {Andreeva, Valentina A. and Unger, Jennifer B.},
Title = {{Determinants of Host Society Acculturation and Its Relationship with
   Health Behaviors and Outcomes: A New Research and Intervention Framework}},
Journal = {{JOURNAL OF IMMIGRANT AND MINORITY HEALTH}},
Year = {{2015}},
Volume = {{17}},
Number = {{5}},
Pages = {{1420-1426}},
Month = {{OCT}},
Abstract = {{Host society acculturation (or reverse acculturation) is a complex,
   multifactorial process reflecting the attitude- and behavior-level
   impact of immigrants on the host society. However, this phenomenon has
   rarely been the subject of systematic research in the area of public
   health. Using qualitative and quantitative findings from different
   health behavior domains, we strove to identify potential individual- and
   environment-level determinants of host society acculturation. Next, we
   developed a context-driven multilevel public health research and
   intervention framework for the study of the relationship between host
   society acculturation and health practices and outcomes. The framework
   posits a number of associations to be evaluated by future
   multidisciplinary research nationally and internationally.}},
