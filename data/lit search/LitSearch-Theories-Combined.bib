@article{ ISI:000082831800006,
Author = {Smith, LR},
Title = {{Intercultural network theory: A cross-paradigmatic approach to
   acculturation}},
Journal = {{INTERNATIONAL JOURNAL OF INTERCULTURAL RELATIONS}},
Year = {{1999}},
Volume = {{23}},
Number = {{4}},
Pages = {{629-658}},
