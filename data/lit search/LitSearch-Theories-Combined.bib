@article{ ISI:000298745100008,
Author = {Bermejo, Isaac and Kriston, Levente and Muthny, Fritz A.},
Title = {{Lay Theories toward Cancer and Myocardial Infarction: A Trans-cultural
   Comparison of Germans and Spaniards}},
Journal = {{JOURNAL OF HEALTH PSYCHOLOGY}},
Year = {{2012}},
Volume = {{17}},
Number = {{1}},
Pages = {{77-86}},
Month = {{JAN}},
Abstract = {{The objective of this study was a transcultural comparison regarding
   illness-related causal and control attributes using a survey of healthy
   Germans in Germany, Spaniards in Spain and Spaniards in Germany. Each
   group contained 100 persons, matched according to sex, age and
   education. Germans showed more pronounced internal causal as well as
   control beliefs while Spaniards in Germany showed fatalistic attribution
   and Spaniards in Spain placed more emphasis on social aspects. It was
   possible to correctly assign over 75 percent of persons to a group
   depending on their characteristics in terms of causal and control
   attributions. Considerations of cultural and migration-related factors
   regarding public health appear especially promising.}},
