@article{ ISI:000438161300003,
Author = {Kim, Hagyun and Hocking, Clare},
Title = {{A Grounded Theory of Korean immigrants' Experiences of Re-Establishing
   Everyday Activities in New Zealand}},
Journal = {{JOURNAL OF IMMIGRANT \& REFUGEE STUDIES}},
Year = {{2018}},
Volume = {{16}},
Number = {{3}},
Pages = {{255-274}},
Abstract = {{This study explored how immigrants locate themselves in a new culture
   through analyzing 25 Korean immigrants' everyday activities in New
   Zealand. The findings suggest that they opted to either behave in Korean
   ways or to take up behaviors reflective of the receiving society, based
   on their level of control over activities disrupted by immigration. The
   findings supplement the concept of acculturation, explaining how
   21st-century immigrants who retain transnational status engage in the
   acculturation process within a globalized context.}},
