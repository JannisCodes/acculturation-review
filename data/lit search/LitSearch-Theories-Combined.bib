@article{ ISI:000620169900009,
Author = {Rodriguez-Garcia, Dan and Luken, Veronica de Miguel and Solana-Solana,
   Miguel},
Title = {{MIXED COUPLES AND THEIR DESCENDANTS IN SPAIN Patterns of mixedness and
   theoretical considerations}},
Journal = {{ANUARIO CIDOB DE LA INMIGRACION}},
Year = {{2020}},
Pages = {{168-195}},
Abstract = {{This paper describes the state of play for mixed unions between
   nationals and foreigners and for their descendants in Spain. Broadly
   termed ``mixticidad{''}, this growing worldwide phenomenon, is beginning
   to be better known in this country. First, a mainly descriptive
   statistical overview is given of the main patterns and evolution (in
   terms of time, type and geographical distribution), both in relation to
   mixed unions and the births resulting from them. Second, the paper
   explores the deeper meanings of this reality using first-hand
   ethnographic information, which allows us to make some reflections on
   its meaning and social consequences in Spanish social inclusion and
   exclusion dynamics. Finally, some conclusions are offered along with the
   main findings.}},
