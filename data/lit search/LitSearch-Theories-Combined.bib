@article{ ISI:000496540100012,
Author = {Tzioka, E. and Fradelos, E. and Aggeli, A.},
Title = {{Cultural adaptation of cognitive behavioral therapy: From the
   theoretical framework to evidence-based practice}},
Journal = {{ARCHIVES OF HELLENIC MEDICINE}},
Year = {{2019}},
Volume = {{36}},
Number = {{6}},
Pages = {{821-829}},
Month = {{NOV-DEC}},
Abstract = {{Increasing trends in migration and a rise in the prevalence of mental
   health disorders have created a need for culturally appropriate
   psychotherapeutic interventions. There is a demand for psychotherapeutic
   services appropriate for the culture and ethnicity of affected
   individuals, and specifically those from the growing population of
   immigrants and asylum seekers. Efforts to culturally adapt cognitive
   behavioral therapy (CBT) regimens have yielded effective procedural
   frameworks for development and application. This is a review of the
   current trends in the development of cultural adaptation frameworks of
   CBT and of the application of the current models of culturally adapted
   CBT.}},
