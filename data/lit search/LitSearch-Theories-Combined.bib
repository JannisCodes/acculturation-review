@article{ ISI:000465480400013,
Author = {Salas-Wright, Christopher P. and Schwartz, Seth J.},
Title = {{The Study and Prevention of Alcohol and Other Drug Misuse Among
   Migrants: Toward a Transnational Theory of Cultural Stress}},
