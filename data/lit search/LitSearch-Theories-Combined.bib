@article{ ISI:000277654300001,
Author = {Schwartz, Seth J. and Unger, Jennifer B. and Zamboanga, Byron L. and
   Szapocznik, Jose},
Title = {{Rethinking the Concept of Acculturation Implications for Theory and
   Research}},
Journal = {{AMERICAN PSYCHOLOGIST}},
Year = {{2010}},
Volume = {{65}},
Number = {{4}},
Pages = {{237-251}},
Month = {{MAY-JUN}},
Abstract = {{This article presents an expanded model of acculturation among
   international migrants and their immediate descendants. Acculturation is
   proposed as a multidimensional process consisting of the confluence
   among heritage-cultural and receiving-cultural practices, values, and
   identifications. The implications of this reconceptualization for the
   acculturation construct, as well as for its relationship to psychosocial
   and health outcomes, are discussed. In particular, an expanded
   operationalization of acculturation is needed to address the ``immigrant
   paradox,{''} whereby international migrants with more exposure to the
   receiving cultural context report poorer mental and physical health
   outcomes. We discuss the role of ethnicity, cultural similarity, and
   discrimination in the acculturation process, offer an operational
   definition for context of reception, and call for studies on the role
   that context of reception plays in the acculturation process. The new
   perspective on acculturation presented in this article is intended to
   yield a fuller understanding of complex acculturation processes and
   their relationships to contextual and individual functioning.}},
