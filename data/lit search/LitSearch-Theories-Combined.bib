@article{ ISI:000410895700004,
Author = {Lee, Kangjae Jerry and Stodolska, Monika},
Title = {{Asian North Americans' Leisure: A Critical Examination of the
   Theoretical Frameworks Used in Research and Suggestions for Future Study}},
Journal = {{LEISURE SCIENCES}},
Year = {{2017}},
Volume = {{39}},
Number = {{6}},
Pages = {{524-542}},
Abstract = {{The aim of this study was to critically examine the theoretical
   frameworks employed in the existing research on Asian North Americans'
   leisure and to offer insights into additional theories that might be
   used in future research on the topic. The study reviewed three major
   theoretical frameworks employed by previous research in the field of
   leisure studies: (1) assimilation and acculturation theory, (2)
   ethnicity-based theories, and (3) self-construal. The study also
   provides recommendations for alternative theoretical approaches that
   could be used in investigations of Asian North Americans' leisure,
   including racial discrimination and stereotyping, the multiple hierarchy
   stratification perspective, transnationalism, and cultural and social
   capital.}},
