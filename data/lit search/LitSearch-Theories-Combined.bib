@article{ ISI:A1996VZ45800003,
Author = {Pries, L},
Title = {{Transnational social spaces. A theoretical-empirical sketch based on
   Mexican-American labor migration}},
Journal = {{ZEITSCHRIFT FUR SOZIOLOGIE}},
Year = {{1996}},
Volume = {{25}},
Number = {{6}},
Pages = {{456-\&}},
Month = {{DEC}},
Abstract = {{During the last two or three decades international migration has changed
   in form and nature, and in the context of general globalization the idea
   of a new `'Age of Migation'' has emerged. International migration is
   less frequently unique and one-directional, and an approach which
   focuses on assimilation or on the simple formation of ethnic minorities
   is no longer adequate. Therefore, more recent concepts like cumulative
   causation, migration networks, transmigrants and transnational
   communities have been discussed. Based on empirical findings from a
   research project in progress (Mexican labor migration from the state of
   Puebla to New York'') and on theoretical considerations along the lines
   of Pierre Bourdieu's `'social spaces,'' a concept of transnational
   social spaces has been developed.}},
