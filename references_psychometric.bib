@Article{Needham2017,
  Author = {Needham, Belinda L. and Mukherjee, Bhramar and Bagchi, Pramita and Kim, Catherine and Mukherjea, Arnab and Kandula, Namratha R. and Kanaya, Alka M.},
  Journal = {Journal of Immigrant and Minority Health},
  Month = {},
  Note = {Publisher: Springer US},
